	pages = {347--360},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\joshr\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\agy6byw4.default\\zotero\\storage\\QZCBVHBB\\840194.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\joshr\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\agy6byw4.default\\zotero\\storage\\JJIT495U\\Spencer et al. - 2000 - Modeling the statistical time and angle of arrival.pdf:application/pdf}
