	year = {2014},
	keywords = {antenna arrays, antenna radiation patterns, Aperiodic array, Approximation methods, Array signal processing, broadband antennas, circular random arrays, cooperative systems, directivity, distributed beamforming, Gaussian distribution, Gaussian element distributions, grating lobes, isotropic wide-band radiators, main beam characteristics, numerical analysis, numerical simulation, Probabilistic logic, Probability density function, random array, Random variables, sidelobe behavior, sidelobe level, spherically-bound random arrays, spherical volume, stochastic mathematical framework, uniform element distributions},
	pages = {3002--3011},
